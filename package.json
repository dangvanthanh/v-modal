{
  "name": "@dangvanthanh/vue-modal",
  "version": "0.1.0",
  "description": "A accessibility modal component for Vue.js",
  "author": "Dang Van Thanh <dangvanthanh@dangthanh.org>",
  "main": "dist/vue-modal.cjs.js",
  "module": "dist/vue-modal.esm.js",
  "unpkg": "dist/vue-modal.umd.js",
  "scripts": {
    "build": "bili src/index.js --bundle-node-modules"
  },
  "keywords": [
    "vue",
    "vue-modal"
  ],
  "license": "MIT",
  "devDependencies": {
    "husky": "1.3.1",
    "lint-staged": "8.1.5",
    "prettier": "1.16.4"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,json,vue},!dist/*.js": [
      "prettier --write",
      "git add"
    ]
  },
  "dependencies": {
    "bili": "4.5.2",
    "rollup-plugin-vue": "4.7.2",
    "vue-template-compiler": "2.6.9"
  }
}
